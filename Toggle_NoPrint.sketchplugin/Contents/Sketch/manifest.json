{
  "author" : "Denis Susilkovsky",
  "authorEmail" : "sudilkovsky@gmail.com",
  "homepage": "https://github.com/sudilkovsky/Toggle-NoPrint-Visibility-Sketch-Plugin",
  "version": 1.2,
  "identifier": "com.example.sketch.toggle-noprint",
  "compatibleVersion": 3,
  "bundleVersion": 1,
  "commands" : [
    {
      "script" : "toggle.cocoascript",
      "handler" : "onRun",
      "shortcut" : "ctrl shift p",
      "name" : "1. Toggle visibility",
      "identifier" : "toggle"
    },
    {
      "script" : "hide.cocoascript",
      "handler" : "onRun",
      "shortcut" : "ctrl cmd shift p",
      "name" : "2. Hide",
      "identifier" : "hide"
    },
    {
      "script" : "lock.cocoascript",
      "handler" : "onRun",
      "shortcut" : "ctrl shift l",
      "name" : "3. Lock",
      "identifier" : "lock"
    },
    {
      "script" : "unlock.cocoascript",
      "handler" : "onRun",
      "shortcut" : "ctrl cmd shift l",
      "name" : "4. Unlock",
      "identifier" : "unlock"
    },
    {
      "script" : "selectall.cocoascript",
      "handler" : "onRun",
      "shortcut" : "ctrl cmd shift a",
      "name" : "5. Select All",
      "identifier" : "selectall"
    }
  ],
  "menu" : {
    "items" : [
      "toggle",
      "hide",
      "lock",
      "unlock",
      "selectall"
    ],
    "title" : "Toggle NoPrint"
  }
}
